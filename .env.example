# Server
AGENT_PORT=8088
NODE_ENV=production

# Security (required)
AGENT_AUTH_TOKEN_CURRENT=replace-me-with-strong-token

# Optional (token rotation)
AGENT_AUTH_TOKEN_PREVIOUS=

# occtl
OCCTL_PATH=/usr/bin/occtl
OCCTL_TIMEOUT_MS=5000
OCCTL_USE_SUDO=true

# RADIUS (optional, defaults shown)
RADIUSCLIENT_CONF_PATH=/etc/radcli/radiusclient.conf
RADIUS_SERVERS_PATH=/etc/radcli/servers
